{
   "name": "project",
   "type": "postgres",
   "host": "project-db-postgresql",
   "port": 5432,
   "username": "project",
   "password": "project",
   "database": "test",
   "synchronize": true,
   "logging": false,
   "cache": {
        "type": "redis",
    "options": {
